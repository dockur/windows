{
  "name": "Windows Server 2008 R2",
  "service": "windows",
  "containerEnv": {
    "VERSION": "2008",
    "RAM_SIZE": "max",
    "DISK_SIZE": "max",
    "CPU_CORES": "max"    
  },  
  "forwardPorts": [8006],  
  "portsAttributes": {
    "8006": {
      "label": "Web",
      "onAutoForward": "openBrowser"
    }
  },
  "otherPortsAttributes": {
    "onAutoForward": "silent"
  },
  "dockerComposeFile": "../codespaces.yml"
}
